      <properties stride="2" filename="neb"> [ potential(0), potential(1), potential(2), potential(3), potential(4), potential(5), potential(6), potential(7), potential(8)   ] </properties>
   </output>
   <prng>
      <seed>18885</seed>
   </prng>
   <system>
      <forces> <force name="driver">driver</force></forces>
      <initialize nbeads="9">
        <file mode="xyz" units="angstrom">./start.xyz</file>
        <cell>
         [   25.29166, 0, 0, 0, 25.29166, 0, 0, 0, 25.29166 ]
        </cell>
      </initialize>
      <mover mode='neb'>
     </mover>
  </system>
</simulation>
